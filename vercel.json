{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.js"
        },
        
            {
              "src": "/dev",
              "dest": "server.js"
            },
          {
              "src":"/api",
              "dest":"server.js"
            }
                     ]
  }
  