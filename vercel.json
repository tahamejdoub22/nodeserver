{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "server.js"
        },
        
            {
              "src": "/dev",
              "dest": "server.js"
            },
          {
              "src":"/api",
              "dest":"server.js"
            }
                     ],
                     "lib": [
                      "ES2018",
                      "ES2017",
                      "es2020",
                      "es3",
                      "es5",
                      "es6",
                      "esnext",
                      "es2015"
                    ]
  }
  